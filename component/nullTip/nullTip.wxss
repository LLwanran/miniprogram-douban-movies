.null-tip {
  height: 400rpx;
  margin: 120rpx 0;
  background-image: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-position: center;
  background-size: 196rpx 128rpx;
  line-height: 800rpx;
  text-align: center;
  color: #999;
}

.null-action {
  margin: 0 auto;
  width: 200rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  border-radius: 8rpx;
  border: 1px solid #ff1268;
  color: #ff1268;
}

.null-action:active {
  background-color: #ff1268;
  color: #fff;
}